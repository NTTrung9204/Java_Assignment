package BaiThucHanh_01;

public class Bai_1 {
	
	int id;
	String name;
	public void insert(int id, String name) {
		this.id = id;
		this.name = name;
	}
	
	public void modify(int id, String name) {
		this.name = name;
	}
	
	public void display() {
		System.out.print(id + " " + name);
	}

	public static void main(String[] args) {
		Bai_1 sv1 = new Bai_1();
		Bai_1 sv2 = new Bai_1();
		
		sv1.insert(1, "PhongNgu");
		sv2.insert(2, "Tu");
		sv2.modify(2, "Khanh");
		sv1.display();
		sv2.display();

	}
	
}
